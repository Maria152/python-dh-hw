   "source": [
    "# Проверяем закон Ципфа\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "relative_frequency = get_relative_frequency(x)\n",
    "plt.plot(range(1, len(relative_frequency) + 1), \n",
    "         [f for w, f in relative_frequency]) \n",
    "plt.show()"
   ]
  },
